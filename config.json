{
  "url": "https://co.vid19.pk/api/cases/feed/?limit=100",
  "database": {
    "db_name": "covid19_pakistan",
    "tables" : {
        "1": {
          "name": "cases",
          "fields": {
            "_id": "int",
            "datetime": "varchar",
            "type": "varchar",
            "reference": "varchar",
            "description": "TEXT",
            "extra": "varchar"
          }
        },
      "2" : {
          "name": "global",
          "fields": {
            "date_reported": "varchar",
            "country_code": "varchar",
            "country": "TEXT",
            "who_region": "varchar",
            "new_cases": "INTEGER",
            "cumulative_cases": "INTEGER",
            "new_deaths": "INTEGER",
            "cumulative_deaths": "INTEGER"
          }
      }
    }
  }
}
