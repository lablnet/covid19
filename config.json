{
  "url": "https://co.vid19.pk/api/cases/feed/?limit=100",
  "database": {
    "db_name": "datasets/covid19_pakistan",
    "tables" : {
        "1": {
          "name": "cases",
          "fields": {
            "_id": "int",
            "datetime": "varchar",
            "type": "varchar",
            "reference": "varchar",
            "description": "TEXT",
            "extra": "varchar"
          }
        },
      "2" : {
          "name": "global",
          "fields": {
            "date_reported": "varchar",
            "country_code": "varchar",
            "country": "TEXT",
            "who_region": "varchar",
            "new_cases": "INTEGER",
            "cumulative_cases": "INTEGER",
            "new_deaths": "INTEGER",
            "cumulative_deaths": "INTEGER"
          }
      },
      "3" : {
          "name": "summery",
          "fields": {
            "datetime": "varchar",
            "total_tests": "varchar",
            "total_cases": "varchar",
            "total_recovered": "varchar",
            "total_deaths": "varchar",
            "total_critical": "varchar",
            "last_tests": "varchar",
            "last_cases": "varchar",
            "last_recovered": "varchar",
            "last_deaths": "varchar",
            "last_critical": "varchar",
            "reference": "varchar"
          }
      },
      "4": {
        "name": "quarantines",
        "fields": {
            "datetime": "varchar",
            "provience": "varchar",
            "name": "varchar",
            "beds": "varchar",
            "reference": "varchar"
          }
      },
      "5": {
        "name": "labs",
        "fields": {
            "datetime": "varchar",
            "provience": "varchar",
            "name": "varchar",
            "city": "varchar",
            "sector": "varchar",
            "reference": "varchar"
          }
      },
      "6" : {
          "name": "vaccine",
          "fields": {
            "datetime": "varchar",
            "total_fully": "varchar",
            "total_partially": "varchar",
            "total_doses": "varchar",
            "last_fully": "varchar",
            "last_partially": "varchar",
            "last_doses": "varchar",
            "reference": "varchar"
          }
      }
    }
  }
}
